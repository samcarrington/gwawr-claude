# Contentful Configuration
# Copy this file to .env and fill in your actual values

# Contentful Space ID
CONTENTFUL_SPACE_ID=your_space_id_here

# Contentful Content Delivery API Access Token (for production content)
CONTENTFUL_ACCESS_TOKEN=your_access_token_here

# Contentful Content Preview API Access Token (for draft content)
CONTENTFUL_PREVIEW_ACCESS_TOKEN=your_preview_token_here

# Environment settings
CONTENTFUL_ENVIRONMENT=master
CONTENTFUL_HOST=cdn.contentful.com
CONTENTFUL_PREVIEW_HOST=preview.contentful.com

# Development settings
NODE_ENV=development